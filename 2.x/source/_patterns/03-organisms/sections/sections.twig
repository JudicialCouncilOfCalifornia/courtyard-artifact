
{% for entry in sections.enteries %}
 
  {% if entry.type == 'section' %}
    {% include '@molecules/section/section.twig' with {
      section: entry
    } %}

  {% elseif entry.type == 'hero' %}
    {% include '@molecules/hero/hero.twig' with {
      hero: entry.hero
    } %}

  {% elseif entry.type == 'accordion' %}
    {% include '@organisms/accordion/accordion.twig' with {
      accordion: entry.accordion
    } %}

  {% elseif entry.type == 'action-list' %}
    {% include '@organisms/cards/cards.twig' with {
      action_list: entry.services
    } %}

  {% elseif entry.type == 'cards' %}
    {% include '@organisms/cards/cards.twig' with {
      cards: entry.cards
    } %}
  {% else %}
    {{ entry }}
  {% endif %}
{% endfor %}
