<div class="jcc-complete">
  <div class="jcc-complete__row">
  {% if complete.icon  %}
  <div class="jcc-complete__icon {{ complete.icon }}">
  </div>
  {% endif %}
  {% if complete.body  %}
  <div class="jcc-complete__left">
    <div class="jcc-complete__body">
      {{ complete.body }}
    </div>
  </div>
  {% endif %}
</div>
</div>
